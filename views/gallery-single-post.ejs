<h1><span><%=style.title%></span></h1>
<div>
	<img src="/<%=style.imageURL%>" alt="<%=style.description%>">
	<div class="article">
		<h4><%=style.title%></h4>
		<p><%=style.description%></p>
		<% if (locals.isLoggedIn) { %>
			<form action="/saveAsFavorite/<%= style._id %>" method="GET">
				<button type="submit" id="send">Save as favorite</button>
				
			</form>
		<% } %>		
	</div>
</div>
</div>
